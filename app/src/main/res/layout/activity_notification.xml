<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationVM" type="com.janraysapplication.app.modules.notification.data.viewmodel.NotificationVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearNotification"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_3">
        <LinearLayout android:id="@+id/linearColumnnotification"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_66pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtNotification"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{notificationVM.notificationModel.txtNotification}"
                style="@style/txtInterbold36" tools:text="@string/lbl_notification"/>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStacklinetwo"
                android:layout_height="@dimen/_79pxv"
                android:layout_marginTop="@dimen/_25pxv" android:layout_width="match_parent">
                <View android:background="@color/gray_200"
                    android:id="@+id/lineLineTwo"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_390pxh"/>
                <LinearLayout android:id="@+id/linearRowgroupfortythree"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_10">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupFortyThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.17"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_16pxv"
                        android:paddingStart="@dimen/_4pxh"
                        android:paddingTop="@dimen/_16pxv"
                        android:singleLine="true"
                        android:text="@{notificationVM.notificationModel.txtGroupFortyThree}"
                        style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_admin"/>
                    <TextView android:id="@+id/txtWehavetransfe"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_weight="0.75"
                        android:layout_width="0dp"
                        android:text="@{notificationVM.notificationModel.txtWehavetransfe}"
                        style="@style/txtInterregular14" tools:text="@string/msg_we_have_transfe"/>
                    <View
                        android:background="@drawable/rectangle_bg_deep_orange_300_border_deep_orange_300_radius_12"
                        android:id="@+id/viewEllipseFourteen"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_39pxh"
                        android:layout_marginTop="@dimen/_31pxv" android:layout_width="@dimen/_24pxh"/>
                </LinearLayout>
            </FrameLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="start"
                android:id="@+id/recyclerNotification"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_6pxh}" tools:itemCount="2" tools:listitem="@layout/row_notification"/>
        </LinearLayout>
        <View android:background="@color/gray_200"
            android:id="@+id/lineLineOne"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_345pxv" android:layout_width="@dimen/_390pxh"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn3512861removeb"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_28pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRow3512861removeb"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImagesremovebg"
                    android:layout_height="@dimen/_41pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_42pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_imagesremovebg"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_imagesremovebg"/>
                <ImageView android:id="@+id/image3512861removeb"
                    android:layout_height="@dimen/_48pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_44pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_3512861removeb"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_3512861removeb"/>
                <ImageView android:id="@+id/imagePngtreedealic"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_width="@dimen/_61pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_pngtreedealic}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_pngtreedealic"/>
                <ImageView android:id="@+id/image8660951"
                    android:layout_height="@dimen/_41pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_41pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_8660951"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_8660951"/>
                <ImageView android:id="@+id/imageV937aew139re"
                    android:layout_height="@dimen/_61pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_58pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_v937aew139re}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_v937aew139re"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowfeed"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFEED"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.11"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationVM.notificationModel.txtFEED}"
                    style="@style/txtIntermedium9" tools:text="@string/lbl_feed"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.33"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationVM.notificationModel.txtLanguage}"
                    style="@style/txtIntermedium9" tools:text="@string/lbl_create_a_post"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDEALS"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.14"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationVM.notificationModel.txtDEALS}"
                    style="@style/txtIntermedium9" tools:text="@string/lbl_deals"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWITHDRAW"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationVM.notificationModel.txtWITHDRAW}"
                    style="@style/txtIntermedium9" tools:text="@string/lbl_withdraw"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPROFILE"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_weight="0.17"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationVM.notificationModel.txtPROFILE}"
                    style="@style/txtIntermedium9" tools:text="@string/lbl_profile"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
